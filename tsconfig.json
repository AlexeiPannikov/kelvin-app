{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "strictNullChecks": false,
    "importHelpers": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "baseUrl": "./",
    "strict": true,
    "paths": {},
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "paths": {
    "@/*": [
      "./src/*"
    ],
    "@assets/*": [
      "./src/assets/*"
    ],
    "@functions/*": [
      "./src/functions/*"
    ],
    "@store/*": [
      "./src/store/*"
    ],
    "@api/*": [
      "./src/api/*"
    ],
    "@view/*": [
      "./src/view/*"
    ]
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
